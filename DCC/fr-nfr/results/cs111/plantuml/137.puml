@startuml
package "Server" {
    package Database {
        abstract class CoordinateGroupDAO{
            + storeCoordinateGroup(coordinateGroup: CoordinateGroup)
            + retrieveCoordinateGroupByIdentifier(identifier: String): CoordinateGroup
            + retrieveAllCoordinateGroups(): List[CoordinateGroup]
            + deleteCoordinateGroupByIdentifier(identifier: String)
        }

      class MySQLDAO extends CoordinateGroupDAO {
          + storeCartesianToPolarConversion()
          + retrievePolarFromLabel(label: String): List[CoordinateGroup]
          + updateCoordinateGroup(coordinateGroup: CoordinateGroup)
          + deleteCoordinateGroupByIdentifier(identifier: String)
      }
      
      class UserDAO { 
          + getUserList() : List[User]
      }
      
  }

class ServerApplication extends Application {
    - databaseConnection: Database // Composition relationship with Database package
    userDao: UserDAO // Aggregation relationship with Database package
    
    coordConversionService: CoordinateGroupConverter
    serverConnection: TCP/IP
    
    + handleCoordinateGroupCreation(coordinateGroup) : void
    + handleCoordinateGroupStoring(coordinateGroup): void
    + handleCoordinateGroupRetrieval(): List[CoordinateGroup]
    + modifyAndSaveCoordinateGroupDetails(coordGroup: CoordinateGroup, updatedValues: String) : void
    + deleteCoordinateGroupByIdentifier(identifier: String): boolean
}

abstract class Application {
        + initialize() : void
        + shutdown() : void
    }
    
    
    abstract class CoordinateGroupConverter {
        - cartesianCoords: (x, y) 
        - polarCoords: (r, theta)
        
        + convertToCartesian(polarGroup: Polar): Cartesian
        + convertToPolar(cartesianGroup: Cartesian): Polar
    }
}

    

package "Client" {
    package InterfacePackage {
        interface UserInterface extends SwingFrame , Server.Application {
            + createCoordinateGroup() : void
            + storeCoordinateGroup(coordinateGroup: CoordinateGroup) : void
            + retrieveAndDisplayCoordinateGroups(): List[CoordinateGroup]
            + searchCoordinateGroupByLabel(label: String): CoordinateGroup
            + modifyCoordinateGroupDetails(coordGroup: CoordinateGroup, updatedValues: String) : void
            + deleteCoordinateGroup(identifier: Integer) : boolean
        }
        
    class DummyCoordConvClientUI extends UserInterface {
        frontendConnection: TCP/IP Connection // Composition relationship with Database package
        dbManagementPackage: Database
        
        - databaseDAO: CoordinateGroupDAO // Aggregation relationship with Database package
    }
    
    
}

}
@enduml