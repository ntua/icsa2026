@startuml
class CoordinateGroup {
    +int id: int
    +String label: String
    +Timestamp timestamp: java.util.Date
    +List<Coordinate> coordinates: List<Coordinate>
}

class CartesianCoord {
    +float x: float
    +float y: float
}

class PolarCoord {
    +float r: float
    +float theta: float
}

class CoordinateManagerClient {
    -SwingApplication: SwingApplication
    +List<UserInput> userInputs: List<UserInput>
    
+void convertToPolar(CoordinateGroup cg) : void
    +void createAndStore() : void
    +void modifyAndSave() : void
    +void deleteById(int id): void
}

class CoordinateManagerServer {
    -MySQLDatabase: MySQL
    -TCPConnection: TCP/IP Connection
    
+void storeCoordinatesInDB(CoordinateGroup cg) : void
    +List<CoordinateGroup> retrieveCoordinatesByLabel(String label): List<CoordinateGroup>
}

class CoordinateConverter {
    +Coordinate convertToPolar(CartesianCoord cartesian) : PolarCoord
    +Cartesian convertToCartesian(PolarCoord polar) : CartesianCoord
}

CoordinateGroup "1" *-- "0..*" ConversionUtilities: CoordinateConverter 
CoordinateGroup "1" -- "1" UserInputs: CoordinateManagerServer
CoordinateGroup "1" -down- "1" StoreInDB: MySQLDatabase
@enduml